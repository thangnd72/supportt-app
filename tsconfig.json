{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./src",
     "paths": {
      "@screens/*": ["screens/*"],
      "@navigation/*":["navigation/*"],
      "@locales/*": ["locales/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@onstants/*": ["constants/*"],
      "@api/*": ["api/*"],
      "@components/*": ["components/*"],
      "@store/*": ["store/*"],      
      "@models/*": ["models/*"],      
     },
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}